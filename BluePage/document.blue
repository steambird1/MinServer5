class __element_style:
	shared maker
	init:
		set this.data=new dict
		set this.expr=""
	function update:
		set m=membersof this.data
		set this.expr=""
		for i=0~m.length:
			set this.expr+=m:i+": "+this.data:(m:i)+";"
	function get key:
		return this.data:key
	function set key value:
		set this.data:key=value
		this.update
	function maker style_desc:
		set tmp=new __element_style
		set re=split style_desc,";",-1
		for i=0~re.length:
			set div=split (re:i),":",1
			set key=trim div.0
			set value=trim div.1
			tmp.set key,value
		return tmp


# Globals or useful only
class __element:
	shared maker
	# Style support
	function style_update:
		set m=membersof this.style_data
		set this.style_expr=""
		for i=0~m.length:
			set this.style_expr+=m:i+": "+this.style_data:(m:i)+";"
	function style_get key:
		return this.style_data:key
	function style_set key value:
		set this.style_data:key=value
		this.style_update
	function style_maker style_desc:
		set this.style_expr=style_desc
		set re=split style_desc,";",-1
		for i=0~re.length:
			set div=split (re:i),":",1
			set key=trim div.0
			set value=trim div.1
			set this.style_data:key=value
	# end
	function maker elem_type elem_id:
		set s=new __element
		set s._type=const elem_type
		set s._id=const elem_id
		s.style_maker (postback.get this._id,"style")
		#.... REQUIRE NEW PROCESSOR
		return s
